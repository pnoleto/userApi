{
    "tokenOptions": {
        "secret": "key value",
        "expiresIn": 3000,
        "algorithm": "HS512",
        "issuer": "userApi"
    },
    "refreshTokenOptions": {
        "secret": "key value to refresh token",
        "expiresIn": 6000,
        "algorithm": "HS512",
        "issuer": "userApi"
    },
    "corsOptions": {
        "origin": [
            "localhost"
        ],
        "methods": [
            "GET",
            "HEAD",
            "PUT",
            "PATCH",
            "POST",
            "DELETE"
        ],
        "allowedHeaders": [
            "Authorization",
            "Content-Type"
        ],
        "optionsSuccessStatus": 200
    },
    "postgresOptions": {
        "host": "localhost",
        "port": 5432,
        "database": "",
        "user": "",
        "password": "",
        "query_timeout": 1000,
        "ssl": false
    },
    "loggerOptions": {
        "meta": false,
        "colorize": true,
        "expressFormat": true,
        "requestWhitelist": [
            "headers",
            "query",
            "body"
        ],
        "responseWhitelist": [
            "headers",
            "query",
            "body"
        ],
        "requestBlacklist": [
            "process",
            "error",
            "trace",
            "stack"
        ],
        "statusLevels": {
            "success": "debug",
            "warn": "debug",
            "error": "info"
        },
        "ignoredRoutes": [
            "/v1"
        ]
    }
}