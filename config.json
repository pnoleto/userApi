{
    "corsOptions": {
        "origin": [
            "localhost"
        ],
        "methods": [
            "GET",
            "HEAD",
            "PUT",
            "PATCH",
            "POST",
            "DELETE"
        ],
        "allowedHeaders": [
            "Authorization",
            "Content-Type"
        ],
        "optionsSuccessStatus": 200
    },
    "loggerOptions": {
        "meta": false,
        "colorize": true,
        "expressFormat": true,
        "requestWhitelist": [
            "headers",
            "query",
            "body"
        ],
        "responseWhitelist": [
            "headers",
            "query",
            "body"
        ],
        "blackFieldsBlacklist": [
            "trace",
            "process",
            "stack",
            "message"
        ],
        "statusLevels": {
            "success": "debug",
            "warn": "debug",
            "error": "info"
        },
        "ignoredRoutes": [
            "/v1"
        ]
    }
}